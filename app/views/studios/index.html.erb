<div class="container">
  <div class="row">
    <div class="col-xs-12">

    <h1>Step 1: Select studio</h1>

      <% @studios.each do |studio| %>
      <%= link_to "#{studio.name}", studio_path(studio) %>
      <p> Address: <%= studio.address%> </p>
      <p> Price: $<%= studio.price%>/hr </p>
      <% end %>

    </div>
  </div>
</div>
